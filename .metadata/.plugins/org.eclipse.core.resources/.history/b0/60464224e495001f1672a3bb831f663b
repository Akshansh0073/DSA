package com.basecodes;



public class BinarySearch {
	
	public static int binarySearchIndex (int arr[], int target) {
		
		int low=0;
		int high = arr.length-1;
		
		while (low<=high) {
			
			int mid = low + (high-low)/2;
			
			if(arr[mid]==target) {
				return mid;
			}
			if(target > arr[mid]) {
				low=mid+1;
			}
			if(target < arr[mid]) {
				high = mid-1;
			}
			
		}
		return -1;
	}
	
	public static void main(String[] args) {
		\
	}
}
